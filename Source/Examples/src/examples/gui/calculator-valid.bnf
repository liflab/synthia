<S>         := <number> <op> <number> <inop> ;
<inop>      := <op> <number> <inop> | = ;
<number>    := <integer> <digit> <rest> | <integer> ;
<integer>   := <nzdigit> <rest> ;
<rest>      := <digit> <rest> | ε ;
<digit>     := 0 | <nzdigit> ;
<nzdigit>   := 1 | 2 | 3 | 4 | 5 | 6 | 7 | 8 | 9 ;
<op>        := + | − | × | ÷ ;